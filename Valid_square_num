import java.util.Scanner; 

class HelloWorld {
    public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    int n  =sc.nextInt();
    int count=0;
    int i=0;
    if(n>1){
    for( i=1;i<=n;i++){
        if(n==i*i){
            count=1;
        if(count==1)
            i=n+1;
        }
    }
    }
    if(count==1){
    System.out.print("true");}
    else
        System.out.print("false");    
           }
}
